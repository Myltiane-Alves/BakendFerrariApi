generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model migrations {
  id        Int    @id @default(autoincrement())
  timestamp BigInt
  name      String @db.VarChar(255)
}

model persons {
  id        Int       @id @default(autoincrement())
  name      String    @db.VarChar(250)
  birthAt   DateTime? @db.Date
  phone     String?   @db.VarChar(16)
  createdAt DateTime  @default(now()) @db.DateTime(0)
  updatedAt DateTime  @default(now()) @db.DateTime(0)
  users     users[]
}

model users {
  id        Int      @id @default(autoincrement())
  email     String   @unique(map: "UQ_97672ac88f789774dd47f7c8be3") @db.VarChar(250)
  password  String   @db.VarChar(255)
  photo     String?  @db.VarChar(255)
  personId  Int
  createdAt DateTime @default(now()) @db.DateTime(0)
  updatedAt DateTime @default(now()) @db.DateTime(0)
  persons   persons  @relation(fields: [personId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "FK_users_persons")

  @@index([personId], map: "FK_users_persons")
}
